{
  "name": "nestjs-nextjs-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:api": "yarn --cwd apps/api start",
    "start:web": "yarn --cwd apps/web start",
    "prisma:migrate:dev": "yarn --cwd apps/api prisma migrate dev",
    "prisma:migrate:deploy": "yarn --cwd apps/api prisma migrate deploy"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
